<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1, shrink-to-fit=no" />
    <meta name="description" content="" />
    <meta name="author" content="" />
    <title>@ViewData["Title"] -Gestor de Projetos</title>
    <!-- Favicon-->
    <link rel="icon" type="image/x-icon" href="assets/favicon.ico" />
    <!-- Font Awesome icons (free version)-->
    <script src="https://use.fontawesome.com/releases/v5.15.4/js/all.js" crossorigin="anonymous"></script>
    <!-- Simple line icons-->
    <link href="https://fonts.googleapis.com/css?family=Montserrat:400,700" rel="stylesheet" type="text/css" />
    <link href="https://fonts.googleapis.com/css?family=Lato:400,700,400italic,700italic" rel="stylesheet" type="text/css" />
    <!-- Core theme CSS (includes Bootstrap)-->
    <link href="~/css/styles.css" rel="stylesheet" />
    <link rel="stylesheet" href="~/lib/bootstrap/dist/css/bootstrap.min.css" />
    <link href="~/css/site.css" rel="stylesheet" />


</head>
<body id="page-top">
    <!-- Navigation-->
    <a class="menu-toggle rounded active"><i class="fas fa-bars"></i></a>
    <nav id="sidebar-wrapper" class="">
        <div class="container">
            <ul class="sidebar-nav">
                <li class="sidebar-nav-item"><a class="nav-link active" asp-action="Index">Gestor de Projetos</a></li>
                <li class="sidebar-nav-item"><a class="nav-link active" asp-action="About">About</a></li>
                <li class="sidebar-nav-item"><a class="nav-link active" asp-action="Blog">Blog</a></li>
                <li class="sidebar-nav-item"><a class="nav-link active" asp-action="Contacts">Contacts</a></li>
                <li class="sidebar-nav-item"><a class="nav-link active" asp-action="TermosCondicoes">Termos e Condições</a></li>
                <li class="sidebar-nav-item"><a class="nav-link active" asp-action="PoliticaPrivacidade">Politica de Privacidade</a></li>
            </ul>
        </div>
    </nav>
    <!-- Header--> 
    <header>
        <nav class="navbar navbar-expand-sm navbar-toggleable-sm navbar-dark bg-black border-bottom box-shadow mb-1">
            <div class="container">
                <h1>
                    <a class="navbar-brand" asp-area="" asp-controller="Home" asp-action="Index">Gestor de Projetos</a>
                </h1>
        </nav>
    </header>

    <div class="container">
        <main role="main" class="pb-3">
            @RenderBody()
        </main>
    </div>

    <!-- Footer-->
    <footer class="container-fluid bg-dark text-white">
        <div class="container">
            <div class="row">
                <!-- Footer do nosso site-->
                <div class="col-lg-3 mb-9 mb-lg-7">
                    <h5 class="text-uppercase mb-4">Sobre a nossa companhia</h5>
                    <p class="lead mb-0">
                        <div class="container">
                            <p>
                                <a class="text-white" asp-area="" asp-controller="Home" asp-action="About">Sobre nos</a>
                            </p>

                            <p>
                                <a class="text-white" asp-area="" asp-controller="Home" asp-action="Blog">Blog</a>
                            </p>

                            <p>
                                <a class="text-white" asp-area="" asp-controller="Home" asp-action="Recursos">Recursos</a>
                            </p>

                            <p>
                                <a class="text-white" asp-area="" asp-controller="Home" asp-action="StatusDoSistema">Status do Sistema</a>
                            </p>

                        </div>

                    </p>
                </div>

                <!-- Footer Recursos -->
                <div class="col-lg-3 mb-9 mb-lg-7">
                    <h5 class="text-uppercase mb-4">Solucoes</h5>
                    <p class="lead mb-0">
                        <div class="container">
                            <p>
                                <a class="text-white" asp-area="" asp-controller="Home" asp-action="Criacao">Criaçao</a>
                            </p>

                            <p>
                                <a class="text-white" asp-area="" asp-controller="Home" asp-action="GerenciamentoDeProjetos">Gerenciamento de Projetos</a>
                            </p>

                            <p>
                                <a class="text-white" asp-area="" asp-controller="Home" asp-action="DesenvolvimentoDeProjetos">Desenvolvimento de Projetos</a>
                            </p>
                        </div>
                    </p>
                </div>

                <div class="col-lg-3 mb-9 mb-lg-7">
                    <h5 class="text-uppercase mb-4">Support</h5>
                    <p class="lead mb-0">
                        <div class="container">
                            <p>
                                <a class="text-white" asp-area="" asp-controller="Home" asp-action="Contact-Support">Contacto do Suporte</a>
                            </p>

                            <p>
                                <a class="text-white" asp-area="" asp-controller="Home" asp-action="Helper-Center">Centro de Ajuda</a>
                            </p>

                            <p>
                                <a class="text-white" asp-area="" asp-controller="Home" asp-action="Videos">Videos</a>
                            </p>
                        </div>
                    </p>
                </div>

                <div class="col-lg-3 mb-9 mb-lg-7">
                    <h5 class="text-uppercase mb-4">Termos e Politicas</h5>
                    <p class="lead mb-0">
                        <div class="container">
                            <p>
                                <a class="text-white" asp-area="" asp-controller="Home" asp-action="TermosCondicoes">Termos e Condições</a>
                            </p>

                            <p>
                                <a class="text-white" asp-area="" asp-controller="Home" asp-action="PoliticaPrivacidade">Politica de Privacidade</a>
                            </p>

                            <p>
                                <a class="text-white" asp-area="" asp-controller="Home" asp-action="InformacaoSegura">Informação Segura</a>
                            </p>
                        </div>
                    </p>
                </div>

            </div>


        </div>

        <div class="copyrightText">
            <div class="siteFooter_secondary_col-wrapper">
                <div class="siteFooter__secondary__col">© <!-- -->2021<!-- --> Gestor de Projetos, Inc. Todos os direitos reservados.</div>
                    <div>
                        <a class="btn btn-outline-light btn-social mx-1" href="https://www.facebook.com/"><i class="fab fa-fw fa-facebook-f"></i></a>
                        <a class="btn btn-outline-light btn-social mx-1" href="https://twitter.com/"><i class="fab fa-fw fa-twitter"></i></a>
                        <a class="btn btn-outline-light btn-social mx-1" href="https://pt.linkedin.com/"><i class="fab fa-fw fa-linkedin-in"></i></a>
                        <a class="btn btn-outline-light btn-social mx-1" href="https://www.pinterest.pt/"><i class="fab fa-fw fa-pinterest"></i></a>
                        <a class="btn btn-outline-light btn-social mx-1" href="https://www.youtube.com/"><i class="fab fa-fw fa-youtube"></i></a>
                    </div>
                
            </div>
        </div>
</footer>
    <!-- Scroll to Top Button-->
    <a class="scroll-to-top rounded" href="#page-top"><i class="fas fa-angle-up"></i></a>
    <!-- Bootstrap core JS-->
    <script src="https://cdn.jsdelivr.net/npm/bootstrap@5.1.3/dist/js/bootstrap.bundle.min.js"></script>
    <!-- Core theme JS-->
    <script src="~/js/scripts.js"></script>

    <script src="~/lib/jquery/dist/jquery.min.js"></script>
    <script src="~/lib/bootstrap/dist/js/bootstrap.bundle.min.js"></script>
    <script src="~/js/site.js" asp-append-version="true"></script>
    @await RenderSectionAsync("Scripts", required: false)



</body>
</html>



